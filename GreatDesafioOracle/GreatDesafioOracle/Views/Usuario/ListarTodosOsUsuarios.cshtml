
@{
    ViewData["Title"] = "Listar Todos Os Usuarios";
    var usuarios = ViewData["Usuarios"] as List<Usuario>;
}

<h1>Listar Todos Os Usuarios</h1>
<table class="table table-striped table-bordered">
    <thead>
        <tr>
            <th>Nome</th>
            <th>CPF</th>
            <th>Data de Cadastro</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in usuarios)
        {
            <tr>
                <td><a href="Usuario/DetalhesUsuario?cpf=@item.Cpf">@item.Nome</a></td>
                <td>@item.Cpf</td>
                <td>@item.Datacadastro</td>
            </tr>
        }
    </tbody>
</table>